# Depends on: libxml2 libgcrypt


packager="sipo"
contributors=""
maintainer="sipo"


description="XML stylesheet transformation library"
url="http://xmlsoft.org/XSLT/"


name=libxslt
version=1.1.33
release=1


source=(ftp://xmlsoft.org/libxml2/$name-$version.tar.gz)

package() {
rm $PKG/usr/lib/lib{e,}xslt.la
rm $PKG/usr/lib/python2.7/site-packages/libxsltmod.la
}
