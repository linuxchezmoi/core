# Depends on:  


packager="sipo"
contributors=""
maintainer="sipo"


description="X11 OpenGL extension wire protocol"
url="https://xorg.freedesktop.org/"


name=glproto
version=1.4.17
release=1


source=(https://xorg.freedesktop.org/releases/individual/proto/glproto-$version.tar.bz2)


build(){
  cd ${name}-${version}
  ./configure --prefix=/usr
  make
}

package() {
  cd ${name}-${version}
  make DESTDIR="${PKG}" install
}
